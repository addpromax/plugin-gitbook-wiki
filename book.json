{
    "title": "MagicBili系列插件WIKI",
    "description": "用来查看相关内容",
    "author": "魔法麦麦",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "plugins": [
         "-lunr", 
         "-search",
         "-sharing",
         "-fontsettings",
         "highlight",
         "livereload",
         "search-pro",
         "expandable-chapters-small",
         "splitter",
         "back-to-top-button",
         "github",
         "hide-element",
         "code",
         "page-toc-button",
         "tbfed-pagefooter",
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/addpromax/plugin-gitbook-wiki"
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
       "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy By MagciBili",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}
